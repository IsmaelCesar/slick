<% new_text_channel = j render partial: 'messaging/text_channel/partials/new', locals: { text_channel: text_channel }, layout: false %>
(function (){
  let modal_container = document.querySelector('#modal-container');
  if(modal_container) {
    modal_container.innerHTML = '<%= new_text_channel %>';
    let modal_new_text_channel_element = document.querySelector('.modal-new-text-channel');
    let modal_new_text_channel = new Modal(modal_new_text_channel_element); 
    modal_new_text_channel.show();
  } 
}).call(this); 